<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="e">

	<select id="event_list" resultType="event">
		select * from event
	</select>
	
	<select id="event_detail" resultType="event" parameterType="int">
		select * from event where event_idx = #{event_idx}
	</select>

</mapper>






















